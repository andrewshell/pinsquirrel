{
  "name": "pinsquirrel",
  "version": "1.0.0",
  "description": "A pnpm monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "CI=true turbo dev",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "format": "turbo format",
    "test": "turbo test",
    "quality": "turbo typecheck && turbo lint && turbo test && turbo format",
    "db:up": "docker compose -f docker-compose.dev.yml down && docker compose -f docker-compose.dev.yml up -d postgres",
    "db:down": "docker compose -f docker-compose.dev.yml down"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "dependencies": {
    "tsx": "^4.20.3"
  }
}
